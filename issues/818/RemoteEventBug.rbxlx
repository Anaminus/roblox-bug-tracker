					<string name="ScriptGuid"></string>
					<ProtectedString name="Source"><![CDATA[local remote = game.ReplicatedStorage:WaitForChild("RemoteEvent")

local uis = game:GetService("UserInputService")
uis.InputBegan:connect(function(inpt)
	if inpt.UserInputType == Enum.UserInputType.MouseButton1 then
		remote:FireServer(math.random())
	end
end)

while true do
	print("WAITING")
	remote.OnClientEvent:wait()
	print("GOT IT")
	wait(5)
end]]></ProtectedString>
