				<string name="ScriptGuid"></string>
				<ProtectedString name="Source"><![CDATA[local remote = game:GetService("ReplicatedStorage").RemoteEvent

--[[ --The error works in both directions - but it can be hard to clearly see if you do both at once.
spawn(function()
	local i = 0
	while true do
		i = (i + 1)%5
		wait(i + .5)
		print("SENT IT")
		remote:FireAllClients()
	end
end)
--]]

while true do
	print("starting loop")
	wait(1)
	print("waiting for input...")
	local plr, num = remote.OnServerEvent:wait()
	print(plr.Name .. " sent " .. num)
	wait(4)
	print("ending loop")
end]]></ProtectedString>
